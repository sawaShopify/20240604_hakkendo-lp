        "heading_eJ9Eei": {
          "type": "heading",
          "settings": {
            "heading": "SDGsへの取り組み",
            "heading_size": "h1"
          }
        },
        "block_image2_aErh6z": {
          "type": "block_image2",
          "settings": {
            "block_image": "shopify:\/\/shop_images\/SDGs_1.png",
            "top_margin": 20,
            "bottom_margin": 50,
            "top_margin_mp": 50,
            "bottom_margin_mp": 50
          }
        },
        "text_AMrP8a": {
          "type": "text",
          "settings": {
            "text": "<p class=\"shadownone\" style=\"text-align: left;\">\n世界全体では、毎年約13億トン（食料生産量の3分の1相当）もの食品が廃棄されており、私たち日本でも、なんと毎年523万トンもの食品ロスが発生しています。<br>\n一方、世界の気候変動にはCO2の排出が大きく影響しているとされ、世界各国ではカーボンニュートラルへの動きが活発化しています。<br>\n<br>\n私たち八恵堂でも、SDGsの活動に貢献できるよう、以下の取り組みを行っています。\n<\/p>",
            "bg_color": false,
            "text_with_title": "",
            "top_margin": 50,
            "bottom_margin": 50,
            "top_margin_mp": 50,
            "bottom_margin_mp": 50
          }
        },
        "image_text3_BLkgWQ": {
          "type": "image-text3",
          "settings": {
            "image1": "shopify:\/\/shop_images\/12_2.png",
            "content1_title": "<p style=\"color: #d29200;\" class=\"f-bigmega\">\nつくる責任・のこさず食べきる責任\n<\/p>",
            "content1_text": "<p class=\"shadownone\">\n特許取得済の冷凍製法で、一切ロスのない食品製造と、旬の野菜を品質を保ったまま冷凍保管できることが八恵堂の強みです。<br>お届け後のご家庭でも冷凍保存を推奨し、「食べたいときに、食べきれる」を実現します。\n<\/p>",
            "image2": "shopify:\/\/shop_images\/13_2.png",
            "content2_title": "<p style=\"color: #417934;\" class=\"f-bigmega\">\nCO2排出削減への貢献\n<\/p>",
            "content2_text": "<p class=\"shadownone\">\n弊社製品パッケージには、ストーンペーパーを採用。<br>\n燃焼時にCO2を排出する紙ではなく、太陽光（日光）で分解され土に還る石灰由来のペーパーです。\n<\/p>",
            "top_margin": 50,
            "bottom_margin": 50,
            "top_margin_mp": 50,
            "bottom_margin_mp": 50
          }
        },
        "text_xF3JKP": {
          "type": "text",
          "settings": {
            "text": "<p style=\"text-align: left;\">\nより繊細な“生きた菌”をお届けする発酵食品製造企業としての責任はもちろんのこと、<br>\n持続可能な生産消費形態と、スマートなビジネスモデルの構築を目指します。\n<\/p>",
            "bg_color": false,
            "text_with_title": "",
            "top_margin": 65,
            "bottom_margin": 50,
            "top_margin_mp": 50,
            "bottom_margin_mp": 50
          }
        }
